extends ../../layout.pug
block contents
    form(method="POST" enctype="multipart/form-data")
        input(id="stylistImg" name="stylistImg" type="file" accept="image/*,.pdf")
        input(id="stylistName" name="stylistName" type="text" placeholder="Write Name" required)
        input(id="stylistSummary" name="stylistSummary" type="text" placeholder="Write Summary")
        input(id="summaryImg" name="summaryImg" type="file" accept="image/*,.pdf")
        button Add Stylist
